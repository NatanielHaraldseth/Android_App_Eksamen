<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/list_view_dato"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="dato"
        android:layout_margin="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/list_view_total_karakter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="T-Karakter"
        android:layout_margin="5dp"
        app:layout_constraintTop_toBottomOf="@+id/list_view_kravpunkt_navn"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/list_view_tekst_no"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="tekst"
        android:layout_margin="5dp"
        app:layout_constraintTop_toBottomOf="@+id/list_view_dato"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/list_view_ordnings_verdi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="O-Verdi"
        android:layout_margin="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_view_tekst_no"/>

    <TextView
        android:id="@+id/list_view_kravpunkt_navn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="kravpunkt"
        android:layout_margin="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_view_ordnings_verdi"/>

</androidx.constraintlayout.widget.ConstraintLayout>
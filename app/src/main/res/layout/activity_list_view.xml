<?xml version = "1.0" encoding = "utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textViewNavn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Navn"
        android:layout_margin="5dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/textViewAdresse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adresse"
        android:layout_margin="5dp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/textViewNavn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/textViewPoststed"/>

    <TextView
        android:id="@+id/textViewPoststed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Poststed"
        android:layout_margin="5dp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/textViewNavn"
        app:layout_constraintStart_toEndOf="@+id/textViewAdresse"
        app:layout_constraintEnd_toEndOf="parent"/>

    <ListView
        android:id="@+id/list"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewPoststed" />

</androidx.constraintlayout.widget.ConstraintLayout>